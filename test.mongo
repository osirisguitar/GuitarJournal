db.Sessions.aggregate(
	{ $match: { userId: ObjectId("512684441ea176ca050002b7") } },
	{ $project: { week: { $week: "$date" } }},
	{ $group: { _id: "$week" , count: { $sum: 1 } } },
	{ $project: { _id: 0, week: "$_id", count: 1 } },
	{ $sort: { week: -1 } },
	{ $limit: 10 },
	{ $sort: { week: 1 } }
)